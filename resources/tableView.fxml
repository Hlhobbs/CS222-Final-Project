<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="parent" alignment="CENTER" maxHeight="500.0" maxWidth="500.0" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.bsu.cs222.tableController">
    <Label fx:id="label" text="Image Table" />
   <TableView fx:id="tableView" maxHeight="500.0" maxWidth="500.0" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0">
     <columns>
         <TableColumn fx:id="hexCol" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" text="Hexadecimal color codes" />
         <TableColumn fx:id="thumbnailCol" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Thumbnail" />
         <TableColumn fx:id="countCol" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" text="Uses of each color" />
     </columns>
   </TableView>
</HBox>
